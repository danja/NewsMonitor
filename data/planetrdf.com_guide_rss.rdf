      <item rdf:about="http://planetrdf.com/guide/#2003-03-05-13:42:00">
        <title>RAP - RDF API for PHP</title>
        <dc:title>RAP - RDF API for PHP</dc:title>
        <link>http://planetrdf.com/guide/#2003-03-05-13:42:00</link>
