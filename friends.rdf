  <rdf:Description rdf:about="http://www.wasab.dk/morten/blog/archives/2004/08/10/wordpress-plugin-seealsos">
    <ns90:purl.org/dc/elements/1.1/creator>Morten Frederiksen</ns90:purl.org/dc/elements/1.1/creator>
    <ns90:purl.org/dc/elements/1.1/date>2004-09-14T21:45:52Z</ns90:purl.org/dc/elements/1.1/date>
    <ns90:purl.org/dc/elements/1.1/subject rdf:resource="http://www.wasab.dk/morten/blog/archives/author/mortenf/skos.rdf#c15"/>
