    <ns90:purl.org/dc/elements/1.1/subject rdf:resource="http://www.wasab.dk/morten/blog/archives/author/mortenf/skos.rdf#c5"/>
    <ns90:purl.org/dc/elements/1.1/subject rdf:resource="http://www.wasab.dk/morten/blog/archives/author/mortenf/skos.rdf#c3"/>
    <ns90:purl.org/dc/elements/1.1/subject>Release</ns90:purl.org/dc/elements/1.1/subject>
    <ns90:purl.org/dc/elements/1.1/subject>WordPress</ns90:purl.org/dc/elements/1.1/subject>
    <ns90:purl.org/dc/elements/1.1/subject rdf:resource="http://www.wasab.dk/morten/blog/archives/author/mortenf/skos.rdf#c27"/>
    <ns90:purl.org/dc/terms/created>2004-08-10T21:28:14Z</ns90:purl.org/dc/terms/created>
    <ns90:purl.org/dc/terms/modified>2004-09-14T21:45:52Z</ns90:purl.org/dc/terms/modified>
    <ns90:purl.org/dc/terms/references rdf:resource="http://b4mad.net/datenbrei/"/>
    <ns90:purl.org/dc/terms/references rdf:resource="http://www.wasab.dk/morten/blog/archives/author/mortenf/foaf.rdf"/>
